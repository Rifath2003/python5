def c(d):
       e = list(set(d))
       if len(e)<2:
           return 'it does not have any second largest number '
       e.sort(reverse=True) 
       return e[1]

a=list(map(int,input("enter numbers sperated by space ").split()))

b=c(a)
print('the second largest number is:',b)

